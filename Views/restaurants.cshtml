<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    @if(Model.Count == 0)
    {
      <h1>No Restaurants</h1>
    }
    <ul>
    @foreach(var restaurant in Model)
    {
      <a href="/restaurants/@restaurant.GetId()"><li>@restaurant.GetName()</li></a>
    }
    </ul>
    <a href="/"><button type="button" name="button">Home</button></a>
  </body>
</html>
