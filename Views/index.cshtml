<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    @if(Model.Count == 0)
    {
      <h1>No Cuisines</h1>
    }
    <ul>
    @foreach(var cuisine in Model)
    {
      <li><a href="/restaurants/cuisine/@cuisine.GetId()">@cuisine.GetCuisineType()</a></li>
    }
    </ul>
    <a href="/restaurants/all"><button type="button" name="button">All Restaurants</button></a>
    <a href="/restaurants/add"><button type="button" name="button">Add Restaurant</button></a>
  </body>
</html>
